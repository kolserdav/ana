{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:react/recommended",
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "eslint-config-prettier",
    "eslint-config-next",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["prettier", "@typescript-eslint", "eslint-plugin-prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": ["node_modules"],
  "rules": {
    "no-unused-vars": 1,
    "prettier/prettier": 1
  }
}
