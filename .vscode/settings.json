{
  "eslint.validate": ["javascript", "typescript"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.workingDirectories": ["app", "server"],
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    ".env.development": "ini",
    ".env.production": "ini",
    ".env.example": "ini",
    ".env": "ini"
  }
}
