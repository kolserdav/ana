{
  "name": "ana",
  "version": "1.24.0",
  "description": "",
  "main": "index.js",
  "homepage": "https://uyem.ru",
  "repository": "git@github.com:kolserdav/ana.git",
  "types": "types",
  "prisma": {
    "schema": "./packages/server/orm/schema.prisma",
    "seed": "node ./scripts/seed/insert-pages.js"
  },
  "scripts": {
    "build:app": "npm run generate && cd packages/app && npm run build",
    "build:server": "npm run migrate && cd packages/server && npm run build",
    "start:server": "cd packages/server && npm run start",
    "start:app": "cd packages/app && npm run start",
    "build": "npm run build:app && npm run build:server",
    "install:node": "npm install --ignore-scripts && npm run install:server && npm run install:app && npm run scripts",
    "install:server": "cd packages/server && npm i",
    "install:app": "cd packages/app && npm i",
    "install:translate": "cd packages/translate2 && pip install -r requirements.txt",
    "migrate:dev": "npm run format && npm run generate && prisma migrate dev --preview-feature",
    "migrate": "npm run generate && prisma migrate deploy --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "seed": "prisma db seed",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "format": "prisma format",
    "translate": "python packages/translate2/main.py",
    "postinstall": "npm run install:node && npm run install:translate",
    "dev:server": "cd packages/server && npm run dev",
    "dev:app": "cd packages/app && npm run dev",
    "dev": "concurrently \"npm run dev:node\" \"npm run translate\"",
    "dev:node": "concurrently \"npm run dev:server\" \"npm run dev:app\"",
    "dev:win": "concurrently \"npm run dev:server\" \"npm run dev:app\"",
    "scripts:create-interfaces-symlink": "node scripts/create-interfaces-symlink.js",
    "scripts:copy-test-constants": "node scripts/copy-test-constants.js",
    "scripts:copy-env": "node scripts/copy-env.js",
    "scripts:git-check": "env-cmd -f ./.env node scripts/git-check.js -e anaserver,ana -p packages/server,packages/app",
    "scripts": "npm run scripts:create-interfaces-symlink && npm run scripts:copy-test-constants && npm run scripts:copy-env",
    "test": "node --test tests/unit/*.test.js",
    "test:translate": "env-cmd -f ./.env node tests/e2e/translate.test.js",
    "types": "tsc -p ./jsconfig.json"
  },
  "keywords": [],
  "author": "kolserdav",
  "license": "AGPL-3.0 license",
  "devDependencies": {
    "@types/html-to-text": "^9.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "^8.2.0",
    "env-cmd": "^10.1.0",
    "es-abstract": "^1.21.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^5.0.0",
    "puppeteer": "^19.6.0",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/http-proxy": "^9.2.1",
    "@prisma/client": "^5.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.3",
    "uuid": "^9.0.0"
  }
}
